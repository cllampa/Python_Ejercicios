from datetime import datetime,time,timedelta

horaActual=datetime.now()
horaSalida=datetime(horaActual.year, horaActual.month,horaActual.day,19,0)
diferencia=horaSalida - horaActual

if horaActual>=horaSalida:
    print("Es hora de irse a casa")
else:
    print(f"Falta {diferencia} para irse a casa")


 

